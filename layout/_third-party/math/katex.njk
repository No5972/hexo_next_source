{%- set katex_uri = theme.vendors.katex or '//unpkg.com/katex@0.15.1/dist/katex.min.css' %}
<link rel="stylesheet" href="{{ url_for(katex_uri) }}">
{%- if theme.math.katex.copy_tex %}
  {%- set copy_tex_js_uri = theme.vendors.copy_tex_js or '//unpkg.com/katex@0.15.1/dist/contrib/copy-tex.min.js' %}
  {%- set copy_tex_css_uri = theme.vendors.copy_tex_css or '//unpkg.com/katex@0.15.1/dist/contrib/copy-tex.min.css' %}
  <script src="{{ url_for(copy_tex_js_uri) }}"></script>
  <link rel="stylesheet" href="{{ url_for(copy_tex_css_uri) }}">
{%- endif %}
